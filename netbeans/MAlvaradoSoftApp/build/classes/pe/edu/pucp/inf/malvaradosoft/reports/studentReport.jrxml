	</parameter>
	<queryString>
		<![CDATA[SELECT
     *,
     student.`idStudent` AS student_idStudent,
     gradexstudent.`idStudent` AS gradexstudent_idStudent,
     gradexstudent.`idGrade` AS gradexstudent_idGrade,
     gradexstudent.`value` AS gradexstudent_value,
     grade.`idGrade` AS grade_idGrade,
     grade.`weight` AS grade_weight,
     grade.`idCourseXSchedule` AS grade_idCourseXSchedule,
     user.`idUser` AS user_idUser,
     user.`names` AS user_names,
     user.`firstLastName` AS user_firstLastName,
     user.`secondLastName` AS user_secondLastName,
     user.`dni` AS user_dni,
     coursexschedule.`idCourseXSchedule` AS coursexschedule_idCourseXSchedule,
     coursexschedule.`idCourse` AS coursexschedule_idCourse,
     course.`idCourse` AS course_idCourse,
     course.`name` AS course_name,
     student.`idClassSection` AS idClassSection
FROM
     `student` student INNER JOIN `gradexstudent` gradexstudent ON student.`idStudent` = gradexstudent.`idStudent`
     INNER JOIN `user` user ON student.`idStudent` = user.`idUser`
     INNER JOIN `grade` grade ON gradexstudent.`idGrade` = grade.`idGrade`
     INNER JOIN `coursexschedule` coursexschedule ON grade.`idCourseXSchedule` = coursexschedule.`idCourseXSchedule`
     INNER JOIN `course` course ON coursexschedule.`idCourse` = course.`idCourse`
   WHERE student.`idStudent` = $P{ms_idAlumno}
     AND student.`idClassSection` = $P{ms_seccion}]]>
	</queryString>
